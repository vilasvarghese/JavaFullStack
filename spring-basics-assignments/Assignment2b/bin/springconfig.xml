<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="country1" class="com.srikanth.fullstackjava.Country">
		<property name="countryId" value="C001"></property>
		<property name="countryName" value="Country1"></property>
	</bean>

	<bean id="country2" class="com.srikanth.fullstackjava.Country">
		<property name="countryId" value="C002"></property>
		<property name="countryName" value="Country2"></property>
	</bean>

	<bean id="player" class="com.srikanth.fullstackjava.Player" scope="prototype">
		<property name="country" ref="country"></property>
	</bean>

	<bean id="country" class="com.srikanth.fullstackjava.Country"></bean>

</beans>
